{
  "name": "student-dashboard",
  "version": "1.0.0",
  "description": "SFU Student Dashboard application",
  "main": "bin/server.js",
  "scripts": {
    "start": "node -r dotenv/config ./bin/server.js",
    "lint": "eslint -c .eslintrc . bin src",
    "lint:fix": "npm run lint -- --fix",
    "lint:changed": "./scripts/lint-changed.sh",
    "test": ". ./scripts/sourceenv.sh .env.test && NODE_ENV=test ava -v -s",
    "test:watch": "npm run test -- -w",
    "deps:reinstall": "rimraf node_modules && npm i",
    "db": "node -r dotenv/config node_modules/.bin/knex --knexfile ./src/server/db/knexfile.js --",
    "db:migrate": "npm run db migrate:latest",
    "db:migrate:rollback": "npm run db migrate:rollback",
    "db:migrate:make": "npm run db migrate:make",
    "db:init": "node -r babel-polyfill -r babel-register -r dotenv/config ./src/server/db/init.js",
    "db:init:test": "npm run db:init -- dotenv_config_path=./.env.test"
  },
  "ava": {
    "files": [
      "src/**/__test__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfu/student-dashboard.git"
  },
  "author": "Graham Ballantyne <grahamb@sfu.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfu/student-dashboard/issues"
  },
  "homepage": "https://github.com/sfu/student-dashboard#readme",
  "pre-commit": [
    "lint:changed"
  ],
  "devDependencies": {
    "ava": "0.14.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "chalk": "1.1.3",
    "eslint": "2.9.0",
    "eslint-plugin-react": "5.0.1",
    "fakeredis": "1.0.3",
    "piping": "0.3.1",
    "pre-commit": "1.1.2",
    "rimraf": "2.5.2",
    "sinon": "1.17.3",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "axios": "0.11.0",
    "babel-eslint": "6.0.4",
    "babel-runtime": "6.6.1",
    "body-parser": "1.15.0",
    "bookshelf": "0.9.4",
    "cas": "github:grahamb/node-cas",
    "connect-redis": "3.0.2",
    "cookie-parser": "1.4.1",
    "dotenv": "2.0.0",
    "eslint": "2.9.0",
    "eslint-plugin-react": "4.2.3",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "helmet": "2.0.0",
    "knex": "0.10.0",
    "lodash": "4.11.1",
    "pg": "4.5.3",
    "piping": "0.3.1",
    "qs": "6.1.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "uid-safe": "2.1.1"
  }
}
